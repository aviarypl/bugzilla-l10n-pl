  [% ELSIF error == "no_bugs_chosen" %]
    [% title = BLOCK %]Nie wybrano żadnych [% terms.bledow %][% END %]
    Nie wybrano żadnych [% terms.bledow %]
    [% IF action == "modify" %]
      do modyfikacji.
    [% ELSIF action == "view" %]
      do przeglądania.
    [% END %]

  [% ELSIF error == "no_bug_ids" %]
    [% title = BLOCK %]Nie wybrano [% terms.bledow %][% END %]
    Nie wybrano żadnych [% terms.bledow %] do
    [% IF action == "add" %] dodania do [% ELSE %] usunięcia z [% END %]
