[%# INTERFACE:
  # name: string; the name of the deleted milestone.
  #
  # product: string; the name of the product the milestone belonged to
  #
  # bug_count: number; the number of bugs which were reassigned to
  #            the default milestone.
  #%]
  
[% title = BLOCK %]Usuwanie wersji docelowej '[% name FILTER html %]' produktu 
                   '[% product FILTER html %]'[% END %]
[% PROCESS global/header.html.tmpl
  title = title
%]

<p>
[% IF bug_count %]
  [% IF bug_count == 1 %]
    Jeden [% terms.blad %] został przepisany
  [% ELSE %]
    [% bug_count %] [% PROCESS ODMIEN c=bug_count o1="" o2=terms.bledy o3=terms.bledow %]
    zostało przepisanych
  [% END %]
  do domyślnej wersji docelowej.

[% ELSE %]
  Żaden [% terms.blad %] nie był przypisany do tej wersji docelowej.
[% END %]
</p>

<p>Wersja docelowa '[% name FILTER html %]' została usunięta.</p>

<p>

[% PROCESS admin/milestones/footer.html.tmpl
  no_edit_milestone_link = 1
 %]

[% PROCESS global/footer.html.tmpl %]
